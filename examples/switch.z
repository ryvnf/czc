printf(^ char, ...) int

main() int {
    switch (9) {
    case 1:
        printf("one\n");
    case 2:
        printf("two\n");
    case 3:
        printf("three\n");
        fallthrough;
    case 4:
        printf("four\n");
    default:
        printf("unknown\n");
    }

    switch ('A') {
    case 'a', 'A':
        printf("A\n");
    case 'b', 'B':
        printf("B\n");
    default:
        printf("?\n");
    }

    return 0;
}
